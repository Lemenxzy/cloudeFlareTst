{
  "name": "ai-chat-system",
  "version": "1.0.0",
  "description": "AI Chat System with React, TypeScript, GraphQL and SSE support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "workers:dev": "wrangler dev",
    "workers:deploy": "wrangler deploy",
    "dev:full": "concurrently \"npm run workers:dev\" \"npm run dev\""
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "graphql": "^16.8.1",
    "highlight.js": "^11.9.0",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "wrangler": "^3.19.0"
  }
}